# example configuration for nursd. run at this dir
# ../../src/nursd rs.conf

[global]

# plugin:
# 1. load the plugins _first_ from the global section
# 2. options for each plugin in seperate section below
plugin	= "@cwd@/.libs/nurs_producer_LISTEN.so"
plugin	= "@cwd@/.libs/nurs_consumer_LOG.so"

# stack
# each element represented id:name, separated by ,
stack	= "listen:LISTEN, log:LOG"

# number of workers
workers	= 8

# number of iosets per stack
iosets	= 8

# each pluginstance settings

[listen]
source = "tcp://127.0.0.1:55555"
